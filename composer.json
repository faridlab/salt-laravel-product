{
  "name": "faridlab/salt-laravel-product",
  "description": "",
  "keywords": ["Salt", "Laravel", "Product"],
  "authors": [
    {
      "name": "Farid Hidayat",
      "email": "farid@startapp.id"
    }
  ],
  "homepage": "https://github.com/faridlab/salt-laravel-product",
  "require": {
    "php": "^8.0.2",
    "laravel/framework": "^10.0",
    "doctrine/dbal": "^3.3",
    "faridlab/salt-laravel": "^10",
    "faridlab/salt-laravel-file": "^10",
    "faridlab/salt-laravel-countries": "^10",
    "zircote/swagger-php": "^4.4",
    "spatie/laravel-permission": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0",
    "phpstan/phpstan": "^0.12",
    "spatie/laravel-ignition": "^2.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SaltProduct\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SaltProduct\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SaltProduct\\Providers\\SaltProductServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
    "analysis": "vendor/bin/phpstan analyse"
  }
}
